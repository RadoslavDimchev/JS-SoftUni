<form class="search" method="GET">
    <input type="search" name="search" value="{{search}}" placeholder="Search here...">
    <button type="submit">Search</button>
</form>
<section id="find-section">
    {{#if housings}}
    {{#each housings }}
    <div class="card">
        <h3>{{name}}</h3>
        <p>{{propertyDescription}}</p>
        <div class="cta-container"><a href="/housing/{{_id}}" class="det-link">Details</a></div>
        <div class="card_circle"><img src="{{image}}"></div>
    </div>
    {{/each}}
    {{else}}
    <div class="no-data-listing">
        <p class="no-offer">No match was found for the submitted type...</p>
    </div>
    {{/if}}
</section>